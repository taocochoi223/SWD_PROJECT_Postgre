{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ConnectionStrings": {
        "DefaultConnection": "YOUR_CONNECTION_STRING_HERE"
    },
    "JwtSettings": {
        "SecretKey": "YOUR_SECRET_KEY_HERE_MUST_BE_LONG_ENOUGH",
        "Issuer": "SWD_IoT_API",
        "Audience": "SWD_IoT_Client",
        "ExpiryMinutes": "60"
    },
    "MqttSettings": {
        "Broker": "mqtt1.eoh.io",
        "Port": 1883,
        "GatewayToken": "YOUR_GATEWAY_TOKEN_HERE",
        "TopicTemplate": "eoh/chip/{0}/third_party/+/data"
    },
    "StatusMonitor": {
        "CheckIntervalSeconds": 60,
        "OfflineThresholdMinutes": 5
    },
    "AllowedHosts": "*"
}